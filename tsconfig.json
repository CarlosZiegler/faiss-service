{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "Node16",

    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
